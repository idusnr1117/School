package s21215_00;

public class PrintfSungjuk {
	
	public static void main(String[] args) {
		
		String name = "홍길동";
		String hakbun = "21200";
		
		int kor = 98;
		int eng = 91;
		int mat = 100;
		
		System.out.printf("성명 : %s, 학번 : %s, 국어 : %d, 수학 : %d, 영어 : %d, 총점 : %d, 평균 : %d", name, hakbun, kor, eng, mat, kor + eng + mat, (kor + eng + mat) / 3);
	}

}
